package com.aurionpro.model;

public class Packet {
	
	IPacketState packet;

	public Packet(IPacketState packet) {
		super();
		this.packet = packet;
	}

	public IPacketState getPacket() {
		return packet;
	}

	public void setPacket(IPacketState packet) {
		this.packet = packet;
	}
	
	public void gotoNextState(){
		packet.next(this);
	}
	
	public void goToPreviousState(){
		packet.previous(this);
	}
	
	public void goToCurrentState(){
		packet.current();
	}

	public void printStatus() {
		// TODO Auto-generated method stub
		packet.current();
	}
	

}
