package com.aurionpro.adapter.model;

public class Chocolate implements IItems {
	
	private String name;
	private int price;

	@Override
	public String getItemName() {
		// TODO Auto-generated method stub
		return this.name;
	}

	@Override
	public int getItemPrice() {
		// TODO Auto-generated method stub
		return  this.price;
	}

}
