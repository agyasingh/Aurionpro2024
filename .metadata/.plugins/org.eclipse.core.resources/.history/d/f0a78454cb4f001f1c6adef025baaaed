package com.aurionpro.adapter.model;

public class Biscuit implements IItems {
	private String name;
	private int price;

	public String getName() {
		return name;
	}

	
	public int getPrice() {
		return price;
	}

	

	@Override
	public String getItemName() {
		// TODO Auto-generated method stub
		return this.name;
	}

	@Override
	public int getItemPrice() {
		// TODO Auto-generated method stub
		return this.price;
	}

}
