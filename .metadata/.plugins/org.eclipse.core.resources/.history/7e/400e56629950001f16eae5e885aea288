package com.aurionpro.model;

public class Packet {
	
	IPacket packet;

	public Packet(IPacket packet) {
		super();
		this.packet = packet;
	}

	public IPacket getPacket() {
		return packet;
	}

	public void setPacket(IPacket packet) {
		this.packet = packet;
	}
	
	public void gotoNextState(){
		packet.next();
	}
	
	public void goToPreviousState(){
		packet.previous();
	}
	
	public void goToCurrentState(){
		packet.current();
	}
	

}
